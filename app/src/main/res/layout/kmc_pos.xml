        <EditText android:id="@+id/dtpobjdate" android:drawableRight="@drawable/calendersmall" android:layout_width="150sp" android:inputType="text" android:layout_height="wrap_content" android:focusable="false" style="@style/customEdit" android:layout_marginRight="10sp" android:textSize="20sp"/>
    </LinearLayout>
    <View android:id="@+id/lineobjdate" android:layout_height="1dip" android:layout_width="fill_parent" android:background="#CCCCCC" />
